generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model prescriptions {
  id                BigInt?
  code              String  @id(map: "prescription_pkey")
  name              String?
  dear              String?
  letter            String?
  concept           String?
  movie             String?
  challengeConcept  String?
  challengeKeyword  String?
  challengeActivity String?
  maintainConcept   String?
  maintainKeyword   String?
  maintainActivity  String?
  reconcileConcept  String?
  reconcileKeyword  String?
  reconcileActivity String?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model results {
  id         Int      @id(map: "user_pkey") @unique(map: "user_id_key") @default(autoincrement())
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @db.Timestamptz(6)
  answers    Json
  code       String?
  nickname   String?
  comment    String?
}

model PRESCRIPTION {
  id                BigInt  @unique
  code              String  @id
  name              String?
  dear              String?
  letter            String?
  concept           String?
  movie             String?
  challengeConcept  String?
  challengeKeyword  String?
  challengeActivity String?
  maintainConcept   String?
  maintainKeyword   String?
  maintainActivity  String?
  reconcileConcept  String?
  reconcileKeyword  String?
  reconcileActivity String?
}
